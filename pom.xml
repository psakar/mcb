	              <goals>
	                <goal>unpack</goal>
	              </goals>
	              <configuration>
	                <artifactItems>
	                  <artifactItem>
	                    <groupId>org.jboss.as</groupId>
	                    <artifactId>jboss-as-dist</artifactId>
	                    <version>${version.org.jboss.jbossas}</version>
