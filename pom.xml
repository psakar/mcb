		<profile>
			<id>install_jboss</id>
			<properties>
				<jboss.download_url>http://download.jboss.org/jbossas/7.1/jboss-as-7.1.1.Final/jboss-as-7.1.1.Final.zip</jboss.download_url>
				<!-- jboss.download_url>file://${basedir}/jboss-as-7.1.1.Final.zip</jboss.download_url -->
				<jboss.zip_filename>jboss-as-7.1.1.Final.zip</jboss.zip_filename>
				<jboss.zip_path>${basedir}/installation</jboss.zip_path>
				<jboss.home>${basedir}/as/jboss</jboss.home>
			</properties>
			<build>
				<plugins>


					<!-- Get AS and put into "target" -->
<!--
 					see https://github.com/ALRubinger/jboss-as-test-example/blob/master/pom.xml
 -->
					<!--
	        <plugin>
	          <groupId>org.apache.maven.plugins</groupId>
