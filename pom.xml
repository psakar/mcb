				 -->

					<plugin>
						<groupId>com.googlecode.maven-download-plugin</groupId>
						<artifactId>maven-download-plugin</artifactId>
						<version>1.0.0</version>
						<executions>
							<execution>
								<id>download-jboss</id>
								<!-- process-test-classes -->
								<phase>pre-integration-test</phase>
								<goals>
									<goal>wget</goal>
								</goals>
								<configuration>
									<url>${jboss.download_url}</url>
									<unpack>true</unpack>
									<cacheDirectory>${jboss.zip_path}</cacheDirectory>
									<outputDirectory>${jboss.home}</outputDirectory>
									<!--
									<unpack>false</unpack>
									<outputDirectory>${jboss.zip_path}</outputDirectory>
									 -->
									<!--
									<md5>df65b5642f33676313ebe4d5b69a3fff</md5>
									<outputFileName>${jboss.zip_filename}</outputFileName>
									<targetDirectory>${jboss.zip_path}</targetDirectory>
									 -->
								</configuration>
							</execution>
						</executions>
					</plugin>
					<plugin>
						<groupId>org.jboss.as.plugins</groupId>
						<artifactId>jboss-as-maven-plugin</artifactId>
						<version>7.4.Final</version>
						<executions>

							<!-- Undeploy the application on clean -->
							<execution>
